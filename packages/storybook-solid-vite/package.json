{
  "name": "@dusksystems/storybook-solid-vite",
  "version": "0.2.0",
  "license": "MIT",
  "keywords": [
    "storybook",
    "solid",
    "solidjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuskSystems/storybook-solid.git",
    "directory": "packages/storybook-solid-vite"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "node": "./dist/node/index.js",
      "import": "./dist/node/index.mjs",
      "require": "./dist/node/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!src",
    "README.md",
    "preset.js"
  ],
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@dusksystems/storybook-solid": "workspace:*",
    "@joshwooding/vite-plugin-react-docgen-typescript": "0.6.0",
    "@storybook/builder-vite": "9.0.0-beta.10"
  },
  "devDependencies": {
    "storybook": "9.0.0-beta.10",
    "tsdown": "0.11.1",
    "typescript": "5.8.3",
    "vite-plugin-solid": "2.11.6"
  },
  "peerDependencies": {
    "solid-js": "^1.9.0",
    "storybook": "^9.0.0-0",
    "vite": "^6.0.0",
    "vite-plugin-solid": "^2.11.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
