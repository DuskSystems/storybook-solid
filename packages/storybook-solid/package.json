{
  "name": "@dusksystems/storybook-solid",
  "version": "0.2.0",
  "license": "MIT",
  "keywords": [
    "storybook",
    "solid",
    "solidjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuskSystems/storybook-solid.git",
    "directory": "packages/storybook-solid"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "import": "./dist/preview.mjs",
      "require": "./dist/preview.js"
    },
    "./preset": "./preset.js",
    "./dist/entry-preview.mjs": "./dist/entry-preview.mjs",
    "./dist/entry-preview-docs.mjs": "./dist/entry-preview-docs.mjs",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!src",
    "README.md",
    "preset.js"
  ],
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch",
    "typecheck": "tsc",
    "test": "vitest --run --coverage --update"
  },
  "dependencies": {
    "@babel/standalone": "7.27.2",
    "@storybook/global": "5.0.0",
    "@storybook/react": "9.0.0-beta.10",
    "prettier": "3.5.3",
    "type-fest": "4.41.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.6.3",
    "@types/babel__standalone": "7.1.9",
    "@types/node": "22.15.17",
    "@vitest/coverage-istanbul": "3.1.3",
    "jsdom": "26.1.0",
    "storybook": "9.0.0-beta.10",
    "tsdown": "0.11.1",
    "typescript": "5.8.3",
    "vite-plugin-solid": "2.11.6",
    "vitest": "3.1.3"
  },
  "peerDependencies": {
    "solid-js": "^1.9.0",
    "storybook": "^9.0.0-0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
