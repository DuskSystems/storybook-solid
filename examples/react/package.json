{
  "name": "@dusksystems/storybook-solid-example-react",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "storybook dev --no-open --port 5005",
    "build": "pnpm build-library && pnpm build-storybook",
    "build-library": "vite build",
    "build-storybook": "storybook build",
    "typecheck": "tsc",
    "test": "vitest --run --coverage",
    "test-watch": "vitest --ui"
  },
  "imports": {
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ]
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "9.0.0-beta.10",
    "@storybook/addon-vitest": "9.0.0-beta.10",
    "@storybook/react-vite": "9.0.0-beta.10",
    "@types/node": "22.15.12",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "@vitejs/plugin-react": "4.4.1",
    "@vitest/browser": "3.1.3",
    "@vitest/coverage-istanbul": "3.1.3",
    "@vitest/ui": "3.1.3",
    "playwright": "1.52.0",
    "storybook": "9.0.0-beta.10",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.1.3"
  }
}
