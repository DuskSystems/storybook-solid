{
  "name": "@dusksystems/storybook-solid-example-solid",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "storybook dev --no-open --port 6006",
    "serve": "http-server storybook-static",
    "build": "pnpm build-library && pnpm build-storybook",
    "build-library": "vite build",
    "build-storybook": "storybook build",
    "typecheck": "tsc",
    "test": "vitest --run --coverage",
    "test-watch": "vitest --ui"
  },
  "imports": {
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ]
  },
  "dependencies": {
    "solid-js": "1.9.6"
  },
  "devDependencies": {
    "@dusksystems/storybook-solid-vite": "workspace:*",
    "@storybook/addon-docs": "9.0.0-beta.10",
    "@storybook/addon-vitest": "9.0.0-beta.10",
    "@types/node": "22.15.12",
    "@vitest/browser": "3.1.3",
    "@vitest/coverage-istanbul": "3.1.3",
    "@vitest/ui": "3.1.3",
    "playwright": "1.52.0",
    "storybook": "9.0.0-beta.10",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-solid": "2.11.6",
    "vitest": "3.1.3"
  }
}
